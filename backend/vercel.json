{
    "version": 2,
    "builds": [
     {
      "src": "./app.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "/events",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
     }
    ]
   }